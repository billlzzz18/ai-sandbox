# AI Agent Sandbox Environment Configuration
# Copy this file to .env and fill in your API keys

# Google Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GOOGLE_CLOUD_PROJECT=your_google_cloud_project_id

# OpenAI API Configuration (for Codex)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Configuration (for Claude Code)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# GitHub API Configuration (for Copilot)
GITHUB_TOKEN=your_github_personal_access_token

# Alibaba Qwen API Configuration
QWEN_API_KEY=your_qwen_api_key_here

# Kilo Code API Configuration
KILO_CODE_API_KEY=your_kilo_code_api_key_here

# Open Code API Configuration
OPEN_CODE_API_KEY=your_open_code_api_key_here

# Server Configuration
PORT=3000

# Development Settings
DEBUG_MODE=true
LOG_LEVEL=info

# Security Settings (change these in production)
SESSION_SECRET=your_secure_random_session_secret
API_RATE_LIMIT=100

# Database Configuration (if using persistent storage)
DATABASE_URL=sqlite:///agents.db

# Cache Configuration
CACHE_TTL=3600
CACHE_SIZE=1000

# Monitoring and Logging
ENABLE_METRICS=true
METRICS_PORT=9090

# External Service URLs (if using external services)
OLLAMA_URL=http://localhost:11434
LOCAL_LLM_ENDPOINT=http://localhost:8000